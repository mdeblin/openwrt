#!/bin/sh

. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
cilab,meshpoint)
	ucidef_add_gpio_switch "poe_out_on" "POE out ON" "1" "0"
	;;
compex,wpj428)
	ucidef_add_gpio_switch "sim_card_select" "SIM card select" "3" "0"
	;;
esac

board_config_flush

exit 0
